  "content": "{\n  \"$schema\": \"https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json\",\n  \"name\": \"HyperCode\",\n  \"scopeName\": \"source.hypercode\",\n  \"patterns\": [{ \"include\": \"#semantic-annotations\" }],\n  \"repository\": {\n    \"semantic-annotations\": {\n      \"patterns\": [\n        {\n          \"name\": \"keyword.control.semantic.hypercode\",\n          \"match\": \"([\\ud83c\\udfaf\\ud83d\\udd0d\\ud83d\\udcdd\\u2705\\u274c\\u26a1\\ud83d\\ude80\\ud83d\\udd27\\ud83d\\udca1\\ud83c\\udfa8\\ud83d\\udd12\\ud83c\\udf10\\ud83d\\udcda\\ud83c\\udf93\\ud83c\\udfd7\\ufe0f\\u2699\\ufe0f\\ud83c\\udfad\\ud83d\\udd2e\\ud83c\\udf1f\\ud83d\\udc8e\\ud83d\\udd25\\ud83d\\udcaf\\ud83c\\udfaa])\\\\s*(@(verifiable|ensures|requires|intent|accessibility|computation|operation|return))\\\\s*\\\\(\",\n          \"captures\": {\n            \"1\": {\n              \"name\": \"punctuation.definition.semantic.hypercode entity.name.semantic.hypercode\"\n            },\n            \"2\": { \"name\": \"keyword.control.semantic.hypercode\" },\n            \"3\": { \"name\": \"entity.name.type.semantic.hypercode\" }\n          }\n        },\n        {\n          \"name\": \"string.quoted.double.semantic.hypercode\",\n          \"begin\": \"([\\ud83c\\udfaf\\ud83d\\udd0d\\ud83d\\udcdd\\u2705\\u274c\\u26a1\\ud83d\\ude80\\ud83d\\udd27\\ud83d\\udca1\\ud83c\\udfa8\\ud83d\\udd12\\ud83c\\udf10\\ud83d\\udcda\\ud83c\\udf93\\ud83c\\udfd7\\ufe0f\\u2699\\ufe0f\\ud83c\\udfad\\ud83d\\udd2e\\ud83c\\udf1f\\ud83d\\udc8e\\ud83d\\udd25\\ud83d\\udcaf\\ud83c\\udfaa])\\\\s*@(verifiable|ensures|requires|intent|accessibility|computation|operation|return)\\\\s*\\\\(\",\n          \"end\": \"\\\\)\",\n          \"patterns\": [\n            {\n              \"name\": \"string.quoted.double.semantic.hypercode\",\n              \"match\": \"\\\\\\\"([^\\\\\\\"]*)\\\\\\\"\"\n            }\n          ]\n        },\n        {\n          \"name\": \"entity.name.function.hypercode\",\n          \"match\": \"\\\\b(function)\\\\s+([a-zA-Z_][a-zA-Z0-9_]*)\",\n          \"captures\": {\n            \"1\": { \"name\": \"keyword.control.function.hypercode\" },\n            \"2\": { \"name\": \"entity.name.function.hypercode\" }\n          }\n        },\n        {\n          \"name\": \"storage.type.return.hypercode\",\n          \"match\": \"->\\\\s*([A-Za-z][A-Za-z0-9_\\\\[\\\\],\\\\s]*)\"\n        }\n      ]\n    }\n  }\n}\n",
