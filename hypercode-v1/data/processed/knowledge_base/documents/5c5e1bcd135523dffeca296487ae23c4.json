  "content": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>HyperCode CodeMap - Interactive Architecture</title>\n    <style>\n        :root {\n            --color-bg-primary: #0a0e27;\n            --color-bg-secondary: #1a1f3a;\n            --color-surface: #252b4a;\n            --color-accent-purple: #b794f6;\n            --color-accent-cyan: #32b8c6;\n            --color-accent-green: #4ade80;\n            --color-accent-orange: #fb923c;\n            --color-accent-red: #f87171;\n            --color-text-primary: #f5f5f5;\n            --color-text-secondary: #a7a9a9;\n            --color-border: rgba(167, 169, 169, 0.3);\n        }\n\n        * { box-sizing: border-box; margin: 0; padding: 0; }\n\n        body {\n            font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;\n            background: var(--color-bg-primary);\n            color: var(--color-text-primary);\n            overflow: hidden;\n        }\n\n        .container {\n            display: flex;\n            height: 100vh;\n        }\n\n        /* Sidebar */\n        .sidebar {\n            width: 280px;\n            background: var(--color-bg-secondary);\n            border-right: 1px solid var(--color-border);\n            padding: 20px;\n            overflow-y: auto;\n        }\n\n        .sidebar h1 {\n            font-size: 24px;\n            margin-bottom: 8px;\n            background: linear-gradient(135deg, var(--color-accent-cyan), var(--color-accent-purple));\n            -webkit-background-clip: text;\n            -webkit-text-fill-color: transparent;\n            background-clip: text;\n        }\n\n        .sidebar .subtitle {\n            font-size: 12px;\n            color: var(--color-text-secondary);\n            margin-bottom: 24px;\n        }\n\n        .filter-section {\n            margin-bottom: 24px;\n        }\n\n        .filter-section h3 {\n            font-size: 14px;\n            font-weight: 600;\n            margin-bottom: 12px;\n            color: var(--color-accent-cyan);\n        }\n\n        .filter-btn {\n            display: flex;\n            align-items: center;\n            width: 100%;\n            padding: 10px 12px;\n            margin-bottom: 8px;\n            background: var(--color-surface);\n            border: 1px solid var(--color-border);\n            border-radius: 8px;\n            color: var(--color-text-primary);\n            font-size: 13px;\n            cursor: pointer;\n            transition: all 0.2s;\n        }\n\n        .filter-btn:hover {\n            background: rgba(50, 184, 198, 0.1);\n            border-color: var(--color-accent-cyan);\n        }\n\n        .filter-btn.active {\n            background: rgba(50, 184, 198, 0.2);\n            border-color: var(--color-accent-cyan);\n        }\n\n        .filter-btn .color-dot {\n            width: 12px;\n            height: 12px;\n            border-radius: 50%;\n            margin-right: 10px;\n        }\n\n        .legend {\n            margin-top: 32px;\n            padding-top: 20px;\n            border-top: 1px solid var(--color-border);\n        }\n\n        .legend h3 {\n            font-size: 12px;\n            font-weight: 600;\n            margin-bottom: 12px;\n            color: var(--color-text-secondary);\n            text-transform: uppercase;\n            letter-spacing: 0.5px;\n        }\n\n        .legend-item {\n            display: flex;\n            align-items: center;\n            font-size: 12px;\n            margin-bottom: 8px;\n            color: var(--color-text-secondary);\n        }\n\n        .legend-item .color-box {\n            width: 16px;\n            height: 16px;\n            border-radius: 4px;\n            margin-right: 8px;\n        }\n\n        /* Canvas Area */\n        .canvas-area {\n            flex: 1;\n            position: relative;\n            overflow: hidden;\n        }\n\n        .canvas {\n            width: 100%;\n            height: 100%;\n            position: relative;\n            cursor: grab;\n        }\n\n        .canvas.grabbing {\n            cursor: grabbing;\n        }\n\n        .canvas-content {\n            position: absolute;\n            width: 3000px;\n            height: 2000px;\n            transform-origin: 0 0;\n        }\n\n        /* Nodes */\n        .node {\n            position: absolute;\n            min-width: 180px;\n            background: var(--color-surface);\n            border: 2px solid;\n            border-radius: 12px;\n            padding: 16px;\n            cursor: pointer;\n            transition: all 0.3s;\n            box-shadow: 0 4px 20px rgba(0, 0, 0, 0.3);\n        }\n\n        .node:hover {\n            transform: scale(1.05);\n            box-shadow: 0 8px 32px rgba(0, 0, 0, 0.5);\n            z-index: 10;\n        }\n\n        .node.core { border-color: var(--color-accent-purple); }\n        .node.ai { border-color: var(--color-accent-cyan); }\n        .node.parsing { border-color: var(--color-accent-green); }\n        .node.devops { border-color: var(--color-accent-orange); }\n        .node.ui { border-color: var(--color-accent-red); }\n\n        .node-header {\n            display: flex;\n            align-items: center;\n            margin-bottom: 8px;\n        }\n\n        .node-icon {\n            width: 24px;\n            height: 24px;\n            border-radius: 6px;\n            margin-right: 10px;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            font-size: 14px;\n        }\n\n        .node-title {\n            font-size: 14px;\n            font-weight: 600;\n        }\n\n        .node-desc {\n            font-size: 11px;\n            color: var(--color-text-secondary);\n            line-height: 1.4;\n        }\n\n        .node-tags {\n            display: flex;\n            flex-wrap: wrap;\n            gap: 4px;\n            margin-top: 8px;\n        }\n\n        .node-tag {\n            font-size: 9px;\n            padding: 2px 6px;\n            border-radius: 4px;\n            background: rgba(255, 255, 255, 0.1);\n        }\n\n        /* Connections */\n        svg.connections {\n            position: absolute;\n            top: 0;\n            left: 0;\n            width: 100%;\n            height: 100%;\n            pointer-events: none;\n            z-index: 0;\n        }\n\n        .connection-line {\n            stroke-width: 2;\n            fill: none;\n            opacity: 0.4;\n        }\n\n        .connection-line.core { stroke: var(--color-accent-purple); }\n        .connection-line.ai { stroke: var(--color-accent-cyan); }\n        .connection-line.parsing { stroke: var(--color-accent-green); }\n        .connection-line.devops { stroke: var(--color-accent-orange); }\n        .connection-line.ui { stroke: var(--color-accent-red); }\n\n        /* Controls */\n        .controls {\n            position: absolute;\n            bottom: 20px;\n            right: 20px;\n            display: flex;\n            gap: 8px;\n            z-index: 100;\n        }\n\n        .control-btn {\n            width: 40px;\n            height: 40px;\n            background: var(--color-surface);\n            border: 1px solid var(--color-border);\n            border-radius: 8px;\n            color: var(--color-text-primary);\n            cursor: pointer;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            font-size: 18px;\n            transition: all 0.2s;\n        }\n\n        .control-btn:hover {\n            background: rgba(50, 184, 198, 0.2);\n            border-color: var(--color-accent-cyan);\n        }\n\n        /* Info Panel */\n        .info-panel {\n            position: absolute;\n            top: 20px;\n            right: 20px;\n            width: 320px;\n            background: var(--color-bg-secondary);\n            border: 1px solid var(--color-border);\n            border-radius: 12px;\n            padding: 20px;\n            display: none;\n            z-index: 100;\n            box-shadow: 0 8px 32px rgba(0, 0, 0, 0.5);\n        }\n\n        .info-panel.visible {\n            display: block;\n        }\n\n        .info-panel h2 {\n            font-size: 18px;\n            margin-bottom: 12px;\n        }\n\n        .info-panel .close-btn {\n            position: absolute;\n            top: 16px;\n            right: 16px;\n            background: none;\n            border: none;\n            color: var(--color-text-secondary);\n            font-size: 20px;\n            cursor: pointer;\n            padding: 4px;\n        }\n\n        .info-section {\n            margin-bottom: 16px;\n        }\n\n        .info-section h4 {\n            font-size: 12px;\n            color: var(--color-accent-cyan);\n            margin-bottom: 8px;\n            text-transform: uppercase;\n        }\n\n        .info-section p {\n            font-size: 13px;\n            line-height: 1.6;\n            color: var(--color-text-secondary);\n        }\n\n        .info-links {\n            display: flex;\n            gap: 8px;\n        }\n\n        .info-link {\n            padding: 8px 12px;\n            background: var(--color-surface);\n            border: 1px solid var(--color-border);\n            border-radius: 6px;\n            color: var(--color-text-primary);\n            text-decoration: none;\n            font-size: 12px;\n            transition: all 0.2s;\n        }\n\n        .info-link:hover {\n            background: rgba(50, 184, 198, 0.2);\n            border-color: var(--color-accent-cyan);\n        }\n    </style>\n</head>\n<body>\n    <div class=\"container\">\n        <!-- Sidebar -->\n        <div class=\"sidebar\">\n            <h1>ðŸš€ HyperCode</h1>\n            <div class=\"subtitle\">Interactive Architecture Map</div>\n\n            <div class=\"filter-section\">\n                <h3>Filter Layers</h3>\n                <button class=\"filter-btn active\" data-layer=\"all\">\n                    <span class=\"color-dot\" style=\"background: linear-gradient(135deg, #b794f6, #32b8c6);\"></span>\n                    Show All Systems\n                </button>\n                <button class=\"filter-btn active\" data-layer=\"core\">\n                    <span class=\"color-dot\" style=\"background: var(--color-accent-purple);\"></span>\n                    Core VM & Runtime\n                </button>\n                <button class=\"filter-btn active\" data-layer=\"ai\">\n                    <span class=\"color-dot\" style=\"background: var(--color-accent-cyan);\"></span>\n                    AI Integration Layer\n                </button>\n                <button class=\"filter-btn active\" data-layer=\"parsing\">\n                    <span class=\"color-dot\" style=\"background: var(--color-accent-green);\"></span>\n                    Parser & Compiler\n                </button>\n                <button class=\"filter-btn active\" data-layer=\"devops\">\n                    <span class=\"color-dot\" style=\"background: var(--color-accent-orange);\"></span>\n                    DevOps & CI/CD\n                </button>\n                <button class=\"filter-btn active\" data-layer=\"ui\">\n                    <span class=\"color-dot\" style=\"background: var(--color-accent-red);\"></span>\n                    UI & Accessibility\n                </button>\n            </div>\n\n            <div class=\"legend\">\n                <h3>Legend</h3>\n                <div class=\"legend-item\">\n                    <div class=\"color-box\" style=\"background: var(--color-accent-purple);\"></div>\n                    Core Logic & VM\n                </div>\n                <div class=\"legend-item\">\n                    <div class=\"color-box\" style=\"background: var(--color-accent-cyan);\"></div>\n                    AI/ML Adapters\n                </div>\n                <div class=\"legend-item\">\n                    <div class=\"color-box\" style=\"background: var(--color-accent-green);\"></div>\n                    Language Tools\n                </div>\n                <div class=\"legend-item\">\n                    <div class=\"color-box\" style=\"background: var(--color-accent-orange);\"></div>\n                    Automation\n                </div>\n                <div class=\"legend-item\">\n                    <div class=\"color-box\" style=\"background: var(--color-accent-red);\"></div>\n                    User Interface\n                </div>\n            </div>\n        </div>\n\n        <!-- Canvas Area -->\n        <div class=\"canvas-area\">\n            <div class=\"canvas\" id=\"canvas\">\n                <svg class=\"connections\" id=\"connections\"></svg>\n                <div class=\"canvas-content\" id=\"canvasContent\"></div>\n            </div>\n\n            <!-- Controls -->\n            <div class=\"controls\">\n                <button class=\"control-btn\" id=\"zoomIn\" title=\"Zoom In\">+</button>\n                <button class=\"control-btn\" id=\"zoomOut\" title=\"Zoom Out\">âˆ’</button>\n                <button class=\"control-btn\" id=\"resetView\" title=\"Reset View\">âŸ²</button>\n            </div>\n\n            <!-- Info Panel -->\n            <div class=\"info-panel\" id=\"infoPanel\">\n                <button class=\"close-btn\" id=\"closeInfo\">Ã—</button>\n                <h2 id=\"infoTitle\">Node Title</h2>\n                <div class=\"info-section\">\n                    <h4>Description</h4>\n                    <p id=\"infoDesc\">Details about this component...</p>\n                </div>\n                <div class=\"info-section\">\n                    <h4>Technologies</h4>\n                    <p id=\"infoTech\">Tech stack info...</p>\n                </div>\n                <div class=\"info-section\">\n                    <h4>Quick Links</h4>\n                    <div class=\"info-links\">\n                        <a href=\"#\" class=\"info-link\" id=\"linkDocs\">ðŸ“š Docs</a>\n                        <a href=\"#\" class=\"info-link\" id=\"linkCode\">ðŸ’» Code</a>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <script>\n        // Node definitions\n        const nodes = [\n            // Core VM & Runtime\n            { id: 'vm-core', type: 'core', title: 'HyperVM Core', desc: 'Main execution engine with spatial memory model', x: 800, y: 400, icon: 'âš¡', tech: 'Rust/C++, Custom bytecode', tags: ['Critical', 'Neurodivergent-Optimized'] },\n            { id: 'memory', type: 'core', title: 'Spatial Memory', desc: '2D/3D addressable memory system', x: 1000, y: 300, icon: 'ðŸ§ ', tech: 'Custom allocation, Visual debugger', tags: ['Visual', 'Core'] },\n            { id: 'executor', type: 'core', title: 'Instruction Executor', desc: 'Multi-paradigm instruction processor', x: 1000, y: 500, icon: 'âš™ï¸', tech: 'JIT compilation, Interpreter fallback', tags: ['Performance'] },\n\n            // AI Integration\n            { id: 'ai-router', type: 'ai', title: 'AI Router', desc: 'Universal adapter for GPT/Claude/Local models', x: 600, y: 200, icon: 'ðŸ¤–', tech: 'REST/gRPC, Async queue', tags: ['Universal', 'Plugin'] },\n            { id: 'ai-codegen', type: 'ai', title: 'Code Generator', desc: 'AI-powered HyperCode generation', x: 400, y: 300, icon: 'âœ¨', tech: 'LLM prompting, AST validation', tags: ['AI-First'] },\n            { id: 'ai-optimize', type: 'ai', title: 'Optimizer', desc: 'AI-driven performance optimization', x: 400, y: 450, icon: 'ðŸŽ¯', tech: 'ML models, Profiling integration', tags: ['Smart'] },\n\n            // Parser & Compiler\n            { id: 'lexer', type: 'parsing', title: 'Lexer', desc: 'Tokenizes HyperCode source', x: 1200, y: 200, icon: 'ðŸ“–', tech: 'Hand-written, Error-tolerant', tags: ['Frontend'] },\n            { id: 'parser', type: 'parsing', title: 'Parser', desc: 'Builds AST from tokens', x: 1400, y: 200, icon: 'ðŸŒ³', tech: 'LL(k), PEG hybrid', tags: ['Frontend'] },\n            { id: 'analyzer', type: 'parsing', title: 'Semantic Analyzer', desc: 'Type checking & validation', x: 1400, y: 350, icon: 'ðŸ”', tech: 'Inference engine, Error recovery', tags: ['Safety'] },\n            { id: 'codegen', type: 'parsing', title: 'Bytecode Gen', desc: 'Emits VM instructions', x: 1200, y: 450, icon: 'ðŸ“¦', tech: 'Optimizing backend', tags: ['Backend'] },\n\n            // DevOps & CI/CD\n            { id: 'ci-pipeline', type: 'devops', title: 'CI/CD Pipeline', desc: 'Automated testing & deployment', x: 800, y: 700, icon: 'ðŸ”„', tech: 'GitHub Actions, Custom runners', tags: ['Automation'] },\n            { id: 'docs-gen', type: 'devops', title: 'Docs Generator', desc: 'Living documentation system', x: 600, y: 800, icon: 'ðŸ“', tech: 'AI-assisted, Auto-update', tags: ['Knowledge'] },\n            { id: 'quality', type: 'devops', title: 'Quality Gate', desc: 'Automated code review & metrics', x: 1000, y: 800, icon: 'âœ…', tech: 'Static analysis, Benchmarks', tags: ['Quality'] },\n\n            // UI & Accessibility\n            { id: 'visual-editor', type: 'ui', title: 'Visual Editor', desc: 'Spatial code editor for neurodivergent minds', x: 1400, y: 600, icon: 'ðŸŽ¨', tech: 'Web/Desktop, Canvas-based', tags: ['Accessible', 'Visual'] },\n            { id: 'debugger', type: 'ui', title: 'Visual Debugger', desc: 'Interactive spatial debugging UI', x: 1200, y: 700, icon: 'ðŸ›', tech: 'Real-time visualization', tags: ['UX'] },\n            { id: 'repl', type: 'ui', title: 'Interactive REPL', desc: 'Instant feedback environment', x: 1400, y: 800, icon: 'ðŸ’¬', tech: 'Web-based, Multi-output', tags: ['Learning'] }\n        ];\n\n        // Connections\n        const connections = [\n            // AI to Core\n            { from: 'ai-router', to: 'vm-core', type: 'ai' },\n            { from: 'ai-codegen', to: 'parser', type: 'ai' },\n            { from: 'ai-optimize', to: 'executor', type: 'ai' },\n\n            // Parsing pipeline\n            { from: 'lexer', to: 'parser', type: 'parsing' },\n            { from: 'parser', to: 'analyzer', type: 'parsing' },\n            { from: 'analyzer', to: 'codegen', type: 'parsing' },\n            { from: 'codegen', to: 'vm-core', type: 'parsing' },\n\n            // Core VM internals\n            { from: 'vm-core', to: 'memory', type: 'core' },\n            { from: 'vm-core', to: 'executor', type: 'core' },\n\n            // UI connections\n            { from: 'visual-editor', to: 'lexer', type: 'ui' },\n            { from: 'debugger', to: 'vm-core', type: 'ui' },\n            { from: 'repl', to: 'vm-core', type: 'ui' },\n\n            // DevOps connections\n            { from: 'ci-pipeline', to: 'quality', type: 'devops' },\n            { from: 'ci-pipeline', to: 'docs-gen', type: 'devops' },\n            { from: 'quality', to: 'vm-core', type: 'devops' }\n        ];\n\n        // State\n        let scale = 1;\n        let offsetX = -600;\n        let offsetY = -100;\n        let isDragging = false;\n        let startX = 0;\n        let startY = 0;\n        let activeFilters = new Set(['all', 'core', 'ai', 'parsing', 'devops', 'ui']);\n\n        // Elements\n        const canvas = document.getElementById('canvas');\n        const canvasContent = document.getElementById('canvasContent');\n        const connectionsSVG = document.getElementById('connections');\n        const infoPanel = document.getElementById('infoPanel');\n\n        // Initialize\n        function init() {\n            renderNodes();\n            renderConnections();\n            setupEventListeners();\n            updateTransform();\n        }\n\n        // Render nodes\n        function renderNodes() {\n            canvasContent.innerHTML = '';\n            nodes.forEach(node => {\n                const nodeEl = document.createElement('div');\n                nodeEl.className = `node ${node.type}`;\n                nodeEl.style.left = node.x + 'px';\n                nodeEl.style.top = node.y + 'px';\n                nodeEl.dataset.id = node.id;\n                nodeEl.dataset.type = node.type;\n\n                const iconBg = {\n                    core: 'var(--color-accent-purple)',\n                    ai: 'var(--color-accent-cyan)',\n                    parsing: 'var(--color-accent-green)',\n                    devops: 'var(--color-accent-orange)',\n                    ui: 'var(--color-accent-red)'\n                }[node.type];\n\n                nodeEl.innerHTML = `\n                    <div class=\"node-header\">\n                        <div class=\"node-icon\" style=\"background: ${iconBg}\">${node.icon}</div>\n                        <div class=\"node-title\">${node.title}</div>\n                    </div>\n                    <div class=\"node-desc\">${node.desc}</div>\n                    <div class=\"node-tags\">\n                        ${node.tags.map(tag => `<span class=\"node-tag\">${tag}</span>`).join('')}\n                    </div>\n                `;\n\n                nodeEl.addEventListener('click', () => showInfo(node));\n                canvasContent.appendChild(nodeEl);\n            });\n        }\n\n        // Render connections\n        function renderConnections() {\n            connectionsE.innerHTML = '';\n            connections.forEach(conn => {\n                const fromNode = nodes.find(n => n.id === conn.from);\n                const toNode = nodes.find(n => n.id === conn.to);\n                if (!fromNode || !toNode) return;\n\n                const fromX = fromNode.x + 90;\n                const fromY = fromNode.y + 60;\n                const toX = toNode.x + 90;\n                const toY = toNode.y + 60;\n\n                const path = document.createElementNS('http://www.w3.org/2000/svg', 'path');\n                const midY = (fromY + toY) / 2;\n                const d = `M ${fromX} ${fromY} C ${fromX} ${midY}, ${toX} ${midY}, ${toX} ${toY}`;\n                \n                path.setAttribute('d', d);\n                path.classList.add('connection-line', conn.type);\n                path.dataset.from = conn.from;\n                path.dataset.to = conn.to;\n                \n                connectionsVG.appendChild(path);\n            });\n        }\n\n        // Show info panel\n        function showInfo(node) {\n            document.getElementById('infoTitle').textContent = node.icon + ' ' + node.title;\n            document.getElementById('infoDesc').textContent = node.desc;\n            document.getElementById('infoTech').textContent = node.tech;\n            infoPanel.classList.add('visible');\n        }\n\n        // Filter handling\n        document.querySelectorAll('.filter-btn').forEach(btn => {\n            btn.addEventListener('click', () => {\n                const layer = btn.dataset.layer;\n                \n                if (layer === 'all') {\n                    if (activeFilters.has('all')) {\n                        activeFilters.clear();\n                        document.querySelectorAll('.filter-btn').forEach(b => b.classList.remove('active'));\n                    } else {\n                        activeFilters = new Set(['all', 'core', 'ai', 'parsing', 'devops', 'ui']);\n                        document.querySelectorAll('.filter-btn').forEach(b => b.classList.add('active'));\n                    }\n                } else {\n                    btn.classList.toggle('active');\n                    if (activeFilters.has(layer)) {\n                        activeFilters.delete(layer);\n                        if (activeFilters.size === 0) {\n                            document.querySelector('[data-layer=\"all\"]').classList.remove('active');\n                        }\n                    } else {\n                        activeFilters.add(layer);\n                        if (activeFilters.size === 5) {\n                            activeFilters.add('all');\n                            document.querySelector('[data-layer=\"all\"]').classList.add('active');\n                        }\n                    }\n                }\n\n                applyFilters();\n            });\n        });\n\n        function applyFilters() {\n            document.querySelectorAll('.node').forEach(node => {\n                const type = node.dataset.type;\n                node.style.display = activeFilters.has(type) || activeFilters.has('all') ? 'block' : 'none';\n            });\n\n            document.querySelectorAll('.connection-line').forEach(line => {\n                const fromNode = nodes.find(n => n.id === line.dataset.from);\n                const toNode = nodes.find(n => n.id === line.dataset.to);\n                const visible = (activeFilters.has(fromNode.type) || activeFilters.has('all')) &&\n                               (activeFilters.has(toNode.type) || activeFilters.has('all'));\n                line.style.display = visible ? 'block' : 'none';\n            });\n        }\n\n        // Pan and zoom\n        function updateTransform() {\n            canvasContent.style.transform = `translate(${offsetX}px, ${offsetY}px) scale(${scale})`;\n        }\n\n        canvas.addEventListener('mousedown', (e) => {\n            if (e.target === canvas || e.target === canvasContent || e.target === connectionsVG) {\n                isDragging = true;\n                startX = e.clientX - offsetX;\n                startY = e.clientY - offsetY;\n                canvas.classList.add('grabbing');\n            }\n        });\n\n        canvas.addEventListener('mousemove', (e) => {\n            if (isDragging) {\n                offsetX = e.clientX - startX;\n                offsetY = e.clientY - startY;\n                updateTransform();\n            }\n        });\n\n        canvas.addEventListener('mouseup', () => {\n            isDragging = false;\n            canvas.classList.remove('grabbing');\n        });\n\n        canvas.addEventListener('mouseleave', () => {\n            isDragging = false;\n            canvas.classList.remove('grabbing');\n        });\n\n        // Zoom controls\n        document.getElementById('zoomIn').addEventListener('click', () => {\n            scale = Math.min(scale * 1.2, 3);\n            updateTransform();\n        });\n\n        document.getElementById('zoomOut').addEventListener('click', () => {\n            scale = Math.max(scale / 1.2, 0.3);\n            updateTransform();\n        });\n\n        document.getElementById('resetView').addEventListener('click', () => {\n            scale = 1;\n            offsetX = -600;\n            offsetY = -100;\n            updateTransform();\n        });\n\n        // Close info panel\n        document.getElementById('closeInfo').addEventListener('click', () => {\n            infoPanel.classList.remove('visible');\n        });\n\n        function setupEventListeners() {\n            // Already set up above\n        }\n\n        // Start\n        init();\n    </script>\n</body>\n</html>\n",
